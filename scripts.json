{
  "allow": ["net", "read"],
  "scripts": {
    "get-images-from-worker": {
      "cmd": "deno run ./get-images-from-worker.ts"
    },
    "get-images": {
      "cmd": "deno run ./get-images.ts"
    },
    "upload-images": {
      "cmd": "deno run ./upload-images.ts"
    },
    "worker:serve-get-images": {
      "cmd": "denoflare serve get-images"
    },
    "worker:publish-get-images": {
      "cmd": "denoflare push get-images"
    },
    "worker:serve-get-cached-image": {
      "cmd": "denoflare serve get-cached-image"
    },
    "worker:publish-get-cached-image": {
      "cmd": "denoflare push get-cached-image"
    }
  }
}
